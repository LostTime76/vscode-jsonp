{
  "name": "vscode-jsonp",
  "displayName": "vscode-jsonp",
  "description": "Syntax highlighting for the jsonp format",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jsonp",
      "aliases": ["JSONP", "jsonp"],
      "extensions": [".jsonp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jsonp",
      "scopeName": "source.jsonp",
      "path": "./syntaxes/jsonp.tmLanguage.json"
    }]
  }
}
